{
    "compilerOptions": {
      "target": "es6",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution":"node",
      "outDir": "./dist",
      "baseUrl": "./src",
    "paths": {
      "@/*": ["*"]
    },
    },
    "ts-node": {
        "swc": true
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  